{
    "name": "yiisoft/yii2-app-advanced",
    "description": "Yii 2 Advanced Project Template",
    "keywords": ["yii2", "framework", "advanced", "project template"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "http://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/yii2"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-debug": "^2.0",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dmstr/yii2-adminlte-asset": "*",
        "yiisoft/yii2-authclient": "^2.0",
        "kartik-v/yii2-widget-typeahead": "*",
        "mootensai/yii2-before-query": "dev-master",
        "yiisoft/yii2-imagine": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "yiisoft/yii2-jui": "^2.0",
        "2amigos/yii2-ckeditor-widget": "~1.0",
        "kartik-v/yii2-widget-colorinput": "*",
        "kartik-v/yii2-widget-timepicker": "*",
        "kartik-v/yii2-dialog": "*",
        "2amigos/yii2-date-picker-widget": "~1.0",
        "kartik-v/yii2-widget-datetimepicker": "*",
        "kartik-v/yii2-widget-datepicker": "@dev",
        "moonlandsoft/yii2-phpexcel": "*",
        "rmrevin/yii2-minify-view": "~1.11",
        "kartik-v/yii2-export": "@dev",
        "kartik-v/yii2-sortable": "*",
        "kartik-v/yii2-krajee-base": "dev-master",
        "himiklab/yii2-sortable-grid-view-widget": "*",
        "richardfan1126/yii2-sortable-gridview": "*",
        "kartik-v/yii2-widget-rating": "*",
        "skeeks/yii2-assets-auto-compress": "*",
        "google/apiclient": "^1.0",
        "facebook/graph-sdk": "^5.4",
        "google/recaptcha": "~1.1",
        "phpseclib/phpseclib": "^2.0",
        "mdmsoft/yii2-admin": "~2.0",
        "google/apiclient-services": "dev-master",
        "kop/yii2-scroll-pager": "dev-master",
        "wadeshuler/yii2-ckeditor-cdn": "~1.0",
        "yiisoft/yii2-elasticsearch": "^2.0@dev",
        "froala/yii2-froala-editor": "^2.7"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }

}
